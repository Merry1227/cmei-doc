#返回类型映射
 @Results(id = "jobApplicationResult", value = {
        @Result(property = "id", column = "id", javaType = Long.class, jdbcType = JdbcType.INTEGER),
        @Result(property = "name", column = "name", javaType = String.class, jdbcType = JdbcType.VARCHAR),
       @Result(property = "category", column = "category", javaType = RecruitCategory.class, jdbcType =JdbcType.INTEGER,typeHandler=org.apache.ibatis.type.EnumOrdinalTypeHandler.class),
       @Result(property = "submitTime", column = "submit_time", javaType = Date.class, jdbcType = JdbcType.DATE),

       